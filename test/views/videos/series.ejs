<div id="series" style="background:url(http://thetvdb.com/banners/<%= series.fanart %>); background-size: cover;">
	<div class="series-information">
		<h1 class="series-name"><%= series.SeriesName %></h1>
		<h2><%= series.Overview %></h2>
	</div>
	<div class="video-list">
		<% _.each(videos, function (video) { %>
				<div class="video-tile">
					<% if (video.episode_metadata) { %>
						<a href="/videos/watch/<%= video.id %>" class="video-link">
							<img src="http://thetvdb.com/banners/<%= video.episode_metadata.filename %>" alt="">
						</a>
						<p class="video-overview">
							<h3><%= video.episode_metadata.EpisodeName %></h3>
							<%= video.episode_metadata.Overview %>
						</p>
					<% } %>
					S<%= video.season %>E<%= video.episode %>
					<div class="clearfix"></div>
				</div>
		<% }); %>
	</div>
</div>